  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^2.0.0",
    "normalize.css": "^4.0.0",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.0.0",
    "react-prop-types": "^0.4.0"
  },
