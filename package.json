{
  "name": "mui-react",
  "version": "0.0.1",
  "description": "mobile web UI build with react, touch devices first",
  "main": "lib/components/index.js",
  "module": "es/components/index.js",
  "jsnext:main": "es/components/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "dist": "webpack --env=dist",
    "ugly": "webpack --env=ugly",
    "build": "babel-node tools/build-cli.js",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "release": "release",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "docs": "node server.js --env=docs",
    "serveDocsBuilt": "node server.js --env=docsBuild",
    "docsBuild": "babel-node tools/build-cli.js --docs-only",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lordisback/mui-react.git"
  },
  "keywords": [
    "react",
    "react-components",
    "web",
    "UI",
    "mobile",
    "touch"
  ],
  "author": "mayingcong <mayingcong@xingxy.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.16.0",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.2.0",
    "child-process-promise": "^2.1.3",
    "colors": "^1.1.2",
    "copyfiles": "^0.2.1",
    "css-loader": "^0.23.0",
    "eslint": "^2.2.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^5.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
